public class Main {
    public static void main(String[] args) {

        System.out.println("Тимур Филиппов");
        int[] nums = {1,2,3,4,5,6,7,8,9,12};
        int sum = 0;
        double mid = 0;
        for (int i = 0;i<nums.length;i++) {
            sum += nums[i];
            mid = (double)sum/nums.length;
        }

        System.out.println("Сумма: " + sum);
        System.out.println("Среднее значение: " + mid);
    }
}
