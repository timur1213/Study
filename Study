import java.util.Arrays;
public class Main {
    public static void main(String[] args) {

        System.out.printf("Timur Filippov!");
        int[] nums = {1,2,3,4,5,6,7,8,9,12};
        int sum = 0;
        float mid = 0;
        for (int i = 0;i<nums.length;i++) {
            sum += nums[i];
            mid = (sum)/nums.length;
        }
            System.out.println(sum);
            System.out.println(mid);
    }
}      
